<div class="container">
  <div class="row">
    <div class="col-6 mx-auto">
      <h2>商品編集</h2></br>
        <%= form_with model: @item, url: admin_item_path(@item), local:true do |f| %>
        <%= render 'items/form_error_message', model: f.object %>
        <%= render "form", f: f %>
        <div class="d-flex justify-content-center">
          <%= f.submit :変更を保存, class: 'btn btn-lg btn btn-outline-success' %>
        </div>
        <% end %>
    </div>
  </div>
</div>